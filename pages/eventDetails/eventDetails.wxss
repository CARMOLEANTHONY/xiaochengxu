page {
  background: #efeff4;
}

.nav-content-images {
  width: 100%;
  height: 420rpx;
  position: relative;
}

.no-p {
  height: 86rpx;
  text-align: center;
  line-height: 86rpx;
  background-color: #fff;
}

.nav-content-images image {
  width: 100%;
  height: 420rpx;
}

button::after {
  border: none;
}

.nav-content-images .button1 {
  position: absolute;
  bottom: 160rpx;
  right: 0px;
  width: 200rpx;
  height: 80rpx;
  background: #9b9b9b;
  padding: 0;
  color: #fff;
  font-size: 16px;
  border-radius: 0;
  border-top-left-radius: 30px;
  border-bottom-left-radius: 30px;
}

.nav-content-images .button2 {
  position: absolute;
  bottom: 40rpx;
  right: 0px;
  width: 200rpx;
  height: 80rpx;
  background: #7ed321;
  padding: 0;
  color: #fff;
  font-size: 16px;
  border-radius: 0;
  border-top-left-radius: 30px;
  border-bottom-left-radius: 30px;
}

.nav-content-images .button3 {
  background: #9b9b9b;
}

.nav-tabar {
  display: flex;
  flex-direction: row;
  text-align: center;
  background: #fff;
  height: 176rpx;
  align-items: center;
}

.nav-tabar-item {
  width: 25%;
  font-size: 0;
}

.nav-tabar-item-images {
  width: 70rpx;
  height: 70rpx;
  margin: 0;
}

.nav-tabar-item-title {
  display: block;
  text-align: center;
  font-size: 12px;
  color: #9b9b9b;
}

.nav-tabar-item-date {
  display: block;
  text-align: center;
  font-size: 8px;
  color: #9b9b9b;
}

.nav-tabar-item-color-active text {
  color: #f4b349;
}

.list {
  width: 100%;
  padding-left: 32rpx;
  background: #fff;
}

.mt {
  margin-top: 20rpx;
}

.list-cell {
  height: 86rpx;
  width: 100%;
  position: relative;
  border-bottom: 1px solid #ebebeb;
}

.list-cell:last-child {
  border-bottom: none;
}

.list-nav-cell {
  height: 86rpx;
  width: 100%;
  position: relative;
  background-color: #efeff4;
  padding-left: 34rpx;
}

.list-nav-cell text {
  color: #888;
  font-size: 28rpx;
  line-height: 86rpx;
}

.list-cell text {
  color: #000;
  font-size: 14px;
  line-height: 43px;
  padding-left: 2rpx;
}

.list-cell image {
  position: absolute;
  right: 30rpx;
  width: 8px;
  height: 14px;
  top: 15px;
}

.list-cell text:last-child {
  color: #888;
  font-size: 14px;
  line-height: 43px;
  position: absolute;
  right: 30rpx;
}

.list-content {
  background: #fff;
  font-size: 14px;
  padding: 30rpx;
}

.list-content-nav {
  text-align: center;
  display: block;
  font-size: 16px;
  font-weight: bold;
}
