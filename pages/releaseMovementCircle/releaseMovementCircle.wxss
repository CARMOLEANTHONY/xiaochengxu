/* pages/releaseMovementCircle/releaseMovementCircle.wxss */
page{
  background: #efeff4;
}
.nav-title {
  line-height: 88rpx;
  font-size: 24rpx;
  padding-left: 30rpx;
  color: #888888;
}
.section {
  background: #fff;
  font-size: 28rpx;
  font-weight: normal;
}
.section input {
  height: 88rpx;
  line-height: 88rpx;
  padding-left: 30rpx;
}
.section textarea {
  padding: 20rpx 30rpx;
}
.images-box {
  display: flex;
  padding: 10rpx 20rpx;
  align-items: center;
  background: #fff;
}
.images-item {
  width: 25%;
  height: 140rpx;
  padding: 0 2.5%;
  position: relative;
}
.images-item .del-images{
  width: 30rpx;
  height: 30rpx;
  position: absolute;
  right: 0rpx;
  top: 0rpx;
}
.images-item .choose-images{
  width: 100%;
  height: 100%;
}
.images-item .add-images{
  width: 100%;
  height: 100%;
}
button::after {
  display: none;
}
.yellow-btn {
  height: 45px;
  margin: 40px 20px 10px;
  background: #f7b64b;
  font-size: 15px;
  padding: 0;
  line-height: 45px;
  color: #fff;
}
.yellow-button-class{
  opacity: 0.6;
}