.ellipsis {
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.nav-box {
  display: flex;
  height: 44px;
  background: #fff;
}

.nav-item {
  width: 33.3%;
  height: 44px;
  display: flex;
  align-items: center;
  justify-content: center;
}

.nav-item text {
  font-size: 16px;
  font-weight: bold;
}

.nav-item-active text {
  font-size: 16px;
  font-weight: bold;
  color: #f4b349;
  border-bottom: 4px #f4b349 solid;
}

.rank-list {
  padding: 20rpx 0;
}

.rank-item {
  background: #fff;
  padding: 10px;
  display: flex;
  align-items: center;
  position: relative;
  border-bottom: 1px #ebebeb solid;
}

.rank-item image {
  width: 300rpx;
  height: 200rpx;
}

.rank-item-left-top-lable {
  position: absolute;
  left: 10px;
  top: 10px;
  height: 0px;
  width: 0px;
  border-width: 40rpx 100rpx;
  border-style: solid;
  border-color: #7ed321 rgba(0, 0, 0, 0) rgba(0, 0, 0, 0) #7ed321;
}

.rank-mask-red {
  border-color: #d0021b rgba(0, 0, 0, 0) rgba(0, 0, 0, 0) #d0021b;
}

.rank-mask-gray {
  border-color: #9b9b9b rgba(0, 0, 0, 0) rgba(0, 0, 0, 0) #9b9b9b;
}

.rank-item-left-top-lable-text {
  position: absolute;
  left: 15px;
  top: 15px;
  font-size: 10px;
  color: #fff;
}

.rank-content {
  margin: 0 20rpx;
  width: 400rpx;
}

.rank-content .item{
  display: block;
  font-size: 13px;
  color: #888;
  line-height: 28rpx;
  margin-bottom: 24rpx;
}

.rank-content text:first-child {
  font-size: 16px;
  line-height: 36rpx;
  margin-bottom: 26rpx;
}

.rank-content text:last-child {
  margin-bottom: 0;
}

.no-data-style {
  text-align: center;
  line-height: 200rpx;
  font-size: 50rpx;
  font-weight: bold;
}
